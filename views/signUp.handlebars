<section class="flex justify-center items-center">
    <form id="form" class="rounded border-blue-500 border-4 flex flex-col ">
        <h3>Username</h3>
        <input class="mx-5" id="username" title="userName" type="text" name="userName">

        <h3>Password</h3>
        <input class="mx-5" id="password" title="password" type="text" name="password" >

        <h3>Email</h3>
        <input class="mx-5" id="email" title="email" type="email" name="email" >

        <button type="submit">Submit</button>
        <p>Want to log in? Click <a href="/login" class="text-indigo-500">here.</a></p>
    </form>
</section>
<script src="/script/signUp.js" ></script>